<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="us.mifeng.activity">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!--往sdcard中写入数据的权限 -->
    <!--在sdcard中创建/删除文件的权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="us.mifeng.activity.AlbumActivity" ></activity>
        <activity android:name="us.mifeng.activity.GalleryActivity"></activity>
        <activity android:name="us.mifeng.activity.ImageFile"></activity>
        <activity android:name="us.mifeng.activity.ShowAllPhoto"></activity>
        <activity android:name=".SeekActivity"></activity>
        <activity android:name=".DetailsActivity"></activity>
        <activity android:name=".SearchActivity"></activity>
        <activity android:name=".MyFaBuActivity"></activity>
        <activity android:name=".MyGuanZhuActivity"></activity>
        <activity android:name=".ShezhiActivity"></activity>
        <activity android:name=".GeRenZiLiao"></activity>
    </application>

</manifest>